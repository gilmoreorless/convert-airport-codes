{
  "name": "Airport code converter",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Do stuff",
  "homepage_url": "https://shoehornwithteeth.com",
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "Expand IATA airport codes"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ]
}